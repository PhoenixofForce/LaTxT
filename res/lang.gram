<S> ::= <BLOCK>
<EXPR> ::= <BLOCK> <EXPR> | STR <STRING_START> <STRING_FOLLOWUP> | <CMD> <STRING_START> <STRING_FOLLOWUP> | ε

<BLOCK> ::= BEGIN <OPTIONS> <ARGS> <EXPR> END <ARGS>
<CMD> ::= ID <OPTIONS> <ARGS>

<OPTIONS> ::= LBRACKET STR RBRACKET | ε
<ARGS> ::= LBRACE STR RBRACE | ε

<STRING_START> ::= STR <STRING_START> | <CMD> <STRING_START> | ε
<STRING_FOLLOWUP> ::= <BLOCK> <EXPR> | ε